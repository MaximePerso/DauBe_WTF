<Window x:Class="DauBe_WTF.SecondaryWindows.Cycles.Cycles"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DauBe_WTF.SecondaryWindows.Cycles"
        xmlns:uc="clr-namespace:DoPE_Cycles_UC;assembly=DoPE_Cycles_UC"
        mc:Ignorable="d"
        Title="Cycles" Height="450" Width="800">
    <Grid>
        <uc:Cycles_UC Height="Auto" Width="Auto" DataContext="{Binding Cycles}"/>
        <Button Command="{Binding StartCycles}" Content="Start" HorizontalAlignment="Left" Margin="10,370,0,0" VerticalAlignment="Top" Width="75" Height="39" />
    </Grid>
</Window>
